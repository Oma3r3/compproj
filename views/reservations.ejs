    }
    
    label {
        display: block;
    }
    
    input {
        border: 1px solid #c4c4c4;
        border-radius: 5px;
        background-color: #fff;
        padding: 3px 5px;
        box-shadow: inset 0 3px 6px rgba(0, 0, 0, 0.1);
        width: 190px;
    }
    
    form button {
        border: 1px solid grey;
        border-radius: 20px;
    }
    
    .container {
        display: flex;
        justify-content: space-around;
        flex-wrap: wrap;
    }
    
    .cards {
        /* border: 1px solid black; */
        background-color: white;
        width: 300px;
        height: 200px;
        display: flex;
        flex-direction: column;
        justify-content: flex-start;
        padding: 10px;
        margin: 20px;
        box-shadow: 0 0 10px white;
    }
    
    #button {
        /* border: 1px solid black; */
        height: 75px;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    
    #button button {
        width: 200px;
        height: 50px;
        font-size: 25px;
        border-radius: 3px;
        border: none;
        box-shadow: 1px 2px 10px gray;
        font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
        background-color: #035e82;
        cursor: pointer;
        color: white;
    }
    
    .hidden {
        display: none;
    }
    
    .datepicker {
        margin: 10px;
    }
    
    nav {
        height: 5rem;
        width: 100vw;
        background-color: #131418;
        box-shadow: 5px 3px 20px rgba(249, 247, 247, 0.2);
        display: flex;
        justify-content: space-between;
        position: fixed;
        z-index: 10;
        font-family: sans-serif;
        letter-spacing: 1px;
        font-weight: 300;
    }
    
    .nav-logo-head {
        display: flex;
        align-items: center;
        padding-left: 1rem;
        gap: 1.2rem;
    }
    
    .logo img {
        height: 3rem;
        filter: contrast(0) brightness(10);
    }
    
    .nav-header {
        color: #00d9ff;
        font-size: x-large;
        /* margin: 1.6rem 0.1rem; */
        font-weight: 500;
    }
    
    .nav-links {
        display: flex;
        list-style: none;
        /* width: 60vw; */
        padding: 0 0.7vw;
        justify-content: end;
        align-items: center;
        text-transform: uppercase;
    }
    
    .nav-links li a {
        color: #f2f5f7;
        text-decoration: none;
        margin: 0 0.7vw;
    }
    
    .nav-links li a:hover {
        color: #61DAFB;
    }
    
    .nav-links li {
        position: relative;
    }
    
    .nav-links li a::before {
        content: "";
        display: block;
        height: 1px;
        width: 0%;
        background-color: #61DAFB;
        position: absolute;
        transition: all ease-in-out 250ms;
        margin: 1.1rem 0 0 10%;
    }
    
    .nav-links li a:hover::before {
        width: 80%;
    }
    
    .login-button {
        color: #f2f5f7;
        background-color: transparent;
        border: 1.5px solid #f2f5f7;
        border-radius: 1.3rem;
        padding: 0.6rem 0.8rem;
        margin-left: 2vw;
        margin-right: 1.8vw;
        font-size: 1rem;
        cursor: pointer;
    }
    
    .login-button:hover {
        color: #131418;
        background-color: #f2f5f7;
        border: 1.5px solid #f2f5f7;
        transition: all ease-in-out 350ms;
    }
    
    .hamburger div {
        width: 30px;
        height: 3px;
        background: #f2f5f7;
        margin: 5px;
        transition: all 1s ease;
    }
    
    .hamburger {
        display: none;
    }
    
    .toppart {
        /* border: 2px solid white; */
        color: white;
        position: relative;
        top: 90px;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }
    
    .header {
        width: 100%;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    
    .container {
        margin-top: 125px;
    }
    
    @media screen and (max-width: 710px) {
        nav {
            position: fixed;
            z-index: 3;
        }
        .hamburger {
            display: block;
            position: absolute;
            cursor: pointer;
            right: 5%;
            top: 50%;
            transform: translate(-5%, -50%);
            z-index: 2;
            transition: all 1s ease;
        }
        .nav-links {
            position: fixed;
            background: #131418;
            height: 30%;
            width: 100%;
            flex-direction: column;
            justify-content: space-around;
            align-items: center;
            clip-path: circle(50px at 90% -20%);
            -webkit-clip-path: circle(50px at 90% -10%);
            transition: all 1s ease-out;
            pointer-events: none;
        }
        .nav-links.open {
            clip-path: circle(1000px at 90% -10%);
            -webkit-clip-path: circle(1000px at 90% -10%);
            pointer-events: all;
        }
        .nav-links li {
            opacity: 0;
        }
        .nav-links li:nth-child(1) {
            transition: all 0.5s ease 0.3s;
        }
        .nav-links li:nth-child(2) {
            transition: all 0.5s ease 0.2s;
        }
        .nav-links li:nth-child(3) {
            transition: all 0.5s ease 0.2s;
        }
        /* .nav-links li:nth-child(4) {
            transition: all 0.5s ease 0.2s;
            margin-left: -0.3em;
        } */
        /* .nav-links li:nth-child(5){
      transition: all 0.5s ease 0.1s;
  }
  .nav-links li:nth-child(6){
      transition: all 0.5s ease 0.1s;
       transform: translateX(-20%);
      margin-left: -1em;
  } */
        /* .nav-links li:nth-child(7){
      transition: all 0.5s ease 1s;
      margin: 0;
  } */
        li.fade {
            opacity: 1;
        }
    }
    
    .toggle .line1 {
        transform: rotate(-45deg) translate(-5px, 6px);
    }
    
    .toggle .line2 {
        transition: all 0.7s ease;
        width: 0;
    }
    
    .toggle .line3 {
        transform: rotate(45deg) translate(-5px, -6px);
    }
    
    @media(max-width: 412px) {
        nav {
            position: fixed;
            z-index: 3;
        }
        .logo img {
            height: 2.5rem;
            transition: height 0.5s ease-in-out 0.1s;
        }
        .nav-header {
            font-size: larger;
            transition: font-size 0.7s ease-in-out 0.2s;
        }
        .header {
            text-align: center;
        }
        .message {
