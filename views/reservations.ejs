    /* Carousel Styling */
    
    .carousel {
        margin-top: 10vh;
        width: 100vw;
        height: 90vh;
        /* background-color: rgb(115, 112, 112);  */
        overflow: hidden;
        position: relative;
        border-bottom: 10px solid #00011e;
    }
    /* Slideshow container */
    
    .slideshow-container {
        max-width: 100%;
        position: relative;
        margin: auto;
    }
    /* Make the images invisible by default */
    
    .Containers {
        display: none;
        /* border: 2px solid red; */
    }
    /* forward & Back buttons */
    
    .Back,
    .forward {
        cursor: pointer;
        position: absolute;
        top: 40%;
        width: auto;
        margin-top: -23px;
        padding: 17px;
        color: #61DAFB;
        font-weight: bold;
        font-size: 40px;
        transition: 0.4s ease;
        border-radius: 0 5px 5px 0;
        user-select: none;
        background-color: rgba(0, 0, 0, 0.4);
    }
    /* Place the "forward button" to the right */
    
    .forward {
        right: 1rem;
        border-radius: 4px 0 0 4px;
    }
    /*when the user hovers,add a black background with some little opacity */
    
    .Back:hover,
    .forward:hover {
        background-color: rgba(0, 0, 0, 0.9);
    }
    
    .Containers img {
        min-width: 900px;
        max-width: 950px;
        height: 90vh;
        image-rendering: crisp-edges;
    }
    /* Caption Info */
    
    .car-club-info {
        min-width: calc(100vw - 950px);
        max-width: calc(100vw - 900px);
        display: flex;
        flex-direction: column;
        justify-content: space-evenly;
        /* gap: 15px; */
        align-items: center;
        border-left: 2px solid black;
        /* background-color: #000000; */
        color: white;
        background-image: url('./images/bg-event-desc-1.jpg');
        background-size: cover;
    }
    
    .car-club-logo {
