        /* width: 100%; */
        font-size: xx-large;
        color: #f2f5f7;
        position: relative;
        width: 50%;
        font-family: sans-serif;
    }
    
    .rw-words span {
        position: absolute;
        left: 0;
        top: 50%;
        opacity: 0;
        overflow: hidden;
        width: 100%;
        color: black;
        /* border: 2px solid white; */
        /* background-color:black; */
    }
    
    .rw-words-2 span {
        animation: rotateWordsSecond 6.5s linear infinite 0s;
    }
    
    .rw-words span:nth-child(2) {
        animation-delay: 1s;
        color: #fff;
    }
    
    .rw-words span:nth-child(3) {
        animation-delay: 2.3s;
        color: #fff;
    }
    
    .rw-words span:nth-child(4) {
        animation-delay: 3.6s;
        color: #fff;
    }
    
    .rw-words span:nth-child(5) {
        animation-delay: 4.9s;
        color: #fff;
    }
    
    .rw-words span:nth-child(6) {
        animation-delay: 6.2s;
        color: #fff;
    }
    
    @keyframes rotateWordsSecond {
        0% {
            opacity: 2;
            animation-timing-function: ease-out;
            width: 0px;
        }
        5% {
            opacity: 1;
            width: 10%;
        }
        10% {
            opacity: 1;
            width: 14%;
        }
        13% {
            opacity: 1;
            width: 17%;
        }
        17% {
            opacity: 1;
            width: 22%;
        }
        30% {
            opacity: 0;
            width: 21%;
        }
        /* 100% { opacity: 0; } */
    }
    
    .rest-head {
        margin-left: 2px;
    }
    
    .recent-head-card {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-evenly;
        gap: 20px;
        width: 100%;
        padding-top: 40px;
        padding-bottom: 40px;
        align-items: center;
    }
    
    .recent-head-card div {
        width: 280px;
        height: 110px;
        border-radius: 10px;
        border: 1px solid black;
        background-color: wheat;
        padding: 15px;
    }
</style>

