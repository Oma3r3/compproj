        margin-top: -5rem;
        width: 100%;
        font-size: 50px;
        font-weight: 700;
        padding: 10px 14px;
        text-align: center;
    }
    /* The circles or bullets and indicators */
    
    .circle-dots {
        /* margin-top: -7px; */
        position: absolute;
        bottom: 20px;
        left: 50%;
        transform: translateX(-32%);
        background-color: #ababab9e;
        padding: 0 10px;
        border-radius: 16px;
    }
    
    .dots {
        cursor: pointer;
        height: 16px;
        width: 16px;
        margin: 5px 10px;
        background-color: rgb(87, 86, 86);
        border-radius: 50%;
        display: inline-block;
        transition: background-color 0.5s ease;
    }
    
    .enable,
    .dots:hover {
        background-color: #282727;
    }
    /* Faint animation */
    
    .fade {
        -webkit-animation-name: fade;
        -webkit-animation-duration: 1.4s;
        animation-name: fade;
        animation-duration: 1.4s;
    }
    
    @-webkit-keyframes fade {
        from {
            opacity: .5
        }
        to {
            opacity: 2
        }
    }
    
    @keyframes fade {
        from {
            opacity: .5
        }
        to {
            opacity: 2
        }
    }
    /* Eventc Sectionnnn */
    
    .recents {
        height: auto;
    }
    
    .recent-body {
        padding: 10px;
        display: flex;
        flex-direction: column;
        /* justify-content: space-between; */
        background-color: #00011e;
    }
    
    .recent-head {
